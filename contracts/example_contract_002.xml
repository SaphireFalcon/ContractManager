<?xml version="1.0" encoding="utf-8"?>
<Contract xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <version>0.2.0</version>
  <uid>example_contract_002</uid>
  <title>Example Contract 002</title>
  <synopsis>This is an example contract, changing all attributes of an orbit.</synopsis>
  <description>Complete the objectives to fulfill this contract. 
Try to achieve all the orbit attributes.</description>
  <isRejectable>false</isRejectable>
  <deadline>3600.0</deadline>
  <prerequisites>
    <Prerequisite>
      <type>maxNumOfferedContracts</type>
      <maxNumOfferedContracts>4</maxNumOfferedContracts>
    </Prerequisite>
    <Prerequisite>
      <type>maxNumAcceptedContracts</type>
      <maxNumAcceptedContracts>2</maxNumAcceptedContracts>
    </Prerequisite>
  </prerequisites>
  <completionCondition>all</completionCondition>
  <requirements>
    <Requirement>
      <uid>change_apoapsis_150_200km</uid>
      <type>orbit</type>
      <title>Change Apoapsis</title>
      <synopsis>Change orbit to 200~220km.</synopsis>
      <description>Change the orbit to a higher orbit with an Apoapsis and Periapsis between 200.000 and 220.000 meter.</description>
      <isHidden>true</isHidden>
      <Orbit>
        <targetBody>Earth</targetBody>
		<type>elliptical</type>
        <minApoapsis>200000</minApoapsis>
        <maxApoapsis>205000</maxApoapsis>
        <minPeriapsis>200000</minPeriapsis>
        <maxPeriapsis>205000</maxPeriapsis>
        <minEccentricity>0.01</minEccentricity>
        <maxEccentricity>0.1</maxEccentricity>
        <minPeriod>5400</minPeriod>
        <maxPeriod>6000</maxPeriod>
        <minLongitudeOfAscendingNode>-360</minLongitudeOfAscendingNode>
        <maxLongitudeOfAscendingNode>360</maxLongitudeOfAscendingNode>
        <minInclination>30</minInclination>
        <maxInclination>180</maxInclination>
        <minArgumentOfPeriapsis>0</minArgumentOfPeriapsis>
        <maxArgumentOfPeriapsis>360</maxArgumentOfPeriapsis>
      </Orbit>
    </Requirement>
  </requirements>
  <actions>
    <Action>
      <trigger>onContractComplete</trigger>
      <type>showMessage</type>
      <showMessage>Congratulations! You pounced the example contract.</showMessage>
    </Action>
    <Action>
      <trigger>onContractFail</trigger>
      <type>showMessage</type>
      <showMessage>Keep persevering; The road to success is pawed with failure.</showMessage>
    </Action>
  </actions>
</Contract>